<Window x:Class="m326.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:m326"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="createUser" Click="createUser_Click" Grid.Column="0" Grid.Row="0">Create User</Button>
        <Button x:Name="findUser" Click="findUser_Click" Grid.Column="0" Grid.Row="1">Find User</Button>
        <Button x:Name="createTopic" Click="createTopic_Click" Grid.Column="1" Grid.Row="0">Create Topic</Button>
        <Button x:Name="updateTopic" Click="updateTopic_Click" Grid.Column="1" Grid.Row="1">Update Topic</Button>
        <Button x:Name="updateCompetence" Click="updateCompetence_Click" Grid.Column="2" Grid.Row="0">Update Competence</Button>
    </Grid>
</Window>
